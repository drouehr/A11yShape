# OpenAI API key (or bearer token from third party provider)
OPENAI_API_KEY=

# override OpenAI-compatible base URL (if using a third party model provider; defaults to OpenAI endpoint)
OPENAI_BASE_URL=

# model ids
OPENAI_MODEL_LIGHT=gpt-5-nano
OPENAI_MODEL_HEAVY=gpt-5-mini

# logging verbosity level; DEBUG / INFO / WARNING / ERROR
LOG_LEVEL=INFO

# API base override. leave blank for local-only, defaults to http://127.0.0.1:3000
# set to tunnel URL for ngrok, e.g. https://<your-subdomain>.ngrok-free.dev
A11YSHAPE_API_BASE=

# path to OpenSCAD, e.g. C:/Program Files/OpenSCAD/openscad.exe (falls back to openscad in PATH, if any)
OPENSCAD_PATH=